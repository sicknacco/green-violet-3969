<h1>Flights Index Page</h1>

<% @flights.each do |flight| %>
  <div id="flight_info_<%= flight.id %>">
    <p>Flight Number: <%= flight.number %></p>
    <p>Airline: <%= flight.airline.name %></p>
    <p>Passengers:</p>
      <% flight.passengers.each do |passenger| %>
        <p><%= passenger.name %></p>
      <% end %>
  </div>
<% end %>